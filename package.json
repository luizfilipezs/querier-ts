{
  "name": "query-ts",
  "version": "0.0.0",
  "description": "Query tool for arrays of objects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luizfilipezs/query-ts.git"
  },
  "keywords": [
    "query",
    "data filtering"
  ],
  "author": {
    "name": "Luiz Filipe da Silva",
    "email": "filipeluiz.bs@gmail.com",
    "url": "https://blogdolipe.com.br"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luizfilipezs/query-ts/issues"
  },
  "homepage": "https://github.com/luizfilipezs/query-ts#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.5.2"
  },
  "files": [
    "lib/**/*"
  ]
}
